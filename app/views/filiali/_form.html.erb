<%= simple_form_for(@filiale) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :codice %>
    <%= f.input :cab %>
    <%= f.input :banca_id %>
    <%= f.input :area_territoriale_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
