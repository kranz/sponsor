<%= simple_form_for(@posizione) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numeroconto %>
    <%= f.input :iban %>
    <%= f.input :richiedente_id %>
    <%= f.input :banca_id %>
    <%= f.input :filiale_id_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
